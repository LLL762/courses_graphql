type Query {
    courseById(id: ID): Course
    courseByName(name: String): Course
    courseByTeacherLastName(lastName: String): [Course]
}

type Course {
    id: Int
    name: String
    description: String
    teacher: Teacher
    degree: Degree
    students: [Student]
}

type Teacher {
    id: Int
    firstName: String
    lastName: String
    email: String
    degrees: [TeacherHasDegree]
}

type Student {
    id: Int
    firstName: String
    lastName: String
    email: String
    degrees: [StudentHasDegree]
}

type Degree {
    id: Int
    name: String
}

type TeacherHasDegree{
    degree: Degree
    earningDate: String
}

type StudentHasDegree{
    degree: Degree
    earningDate: String
}





